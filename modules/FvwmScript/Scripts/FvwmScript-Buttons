  SingleClic :
  Begin
   If (GetValue 9)==1 Then
    Do {Exec textedit -En 30 &}
   If (GetValue 9)==2 Then
    Do {Exec medit &}
   If (GetValue 9)==3 Then
    Do {Exec xedit &}
   If (GetValue 9)==4 Then
    Do {Exec coaxe &}
   If (GetValue 9)==5 Then
    Do {Exec bash -c "netscape" &}
   If (GetValue 9)==6 Then
    Do {Exec xclipboard &}
   If (GetValue 9)==7 Then
    Do {Exec plan &}
  End
End

